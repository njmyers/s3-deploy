{
  "name": "s3-deploy-scripts",
  "version": "0.1.5",
  "description": "deploy static sites to amazon s3",
  "main": "script.js",
  "scripts": {
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "build": "webpack",
    "create-index": "node scripts/create-index.js",
    "watch": "webpack --watch",
    "test": "jest --watchAll",
    "prettier": "prettier --write '**/*.{js,jsx,json}'",
    "jest": "jest",
    "precommit": "lint-staged"
  },
  "author": "Nick Myers",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "aws-sdk": "^2.195.0",
    "bluebird": "^3.5.1",
    "camelcase": "^4.1.0",
    "chalk": "^2.3.1",
    "commander": "^2.15.1",
    "dotenv": "^4.0.0",
    "global": "^4.3.2",
    "lodash": "^4.17.5",
    "mime-types": "^2.1.17",
    "smalldash": "^1.0.2"
  },
  "bin": {
    "s3-deploy": "script.js"
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "__tests__/helpers"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.74.0",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "lint-staged": "^6.1.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "shebang-loader": "^0.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^2",
    "webpack-node-externals": "^1.7.2"
  }
}

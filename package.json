{
	"name": "s3-deploy-scripts",
	"version": "0.1.4",
	"description": "deploy static sites to amazon s3",
	"main": "deploy-release.js",
	"directories": {
		"lib": "lib"
	},
	"scripts": {
		"patch-release": "npm version patch && npm publish && git push --follow-tags",
		"test": "jest --watchAll",
		"prettier": "prettier --write '**/*.{js,jsx,json}'",
		"jest": "jest",
		"precommit": "lint-staged && npm test"
	},
	"author": "Nick Myers",
	"license": "ISC",
	"dependencies": {
		"aws-sdk": "^2.195.0",
		"bluebird": "^3.5.1",
		"camelcase": "^4.1.0",
		"chalk": "^2.3.1",
		"dotenv": "^4.0.0",
		"jest": "^22.3.0",
		"lodash": "^4.17.5",
		"mime-types": "^2.1.17",
		"npm-run-all": "^4.1.2"
	},
	"bin": {
		"s3-deploy": "s3-deploy.js"
	},
	"lint-staged": {
		"**/*.{js,jsx,json}": ["prettier --write", "git add"]
	},
	"devDependencies": {
		"husky": "^0.14.3",
		"lint-staged": "^6.1.0",
		"prettier": "^1.10.2"
	}
}
